{
  "name": "upline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "prettier --write .",
    "test": "jest",
    "test:fix": "jest -u",
    "test:watch": "jest --watch",
    "types:check": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "schema:download": "apollo client:download-schema --config=apollo.config.js src/graphql/types/graphql-schema.json",
    "schema:codegen": "apollo client:codegen --config=apollo.config.js --localSchemaFile=src/graphql/types/graphql-schema.json --target=typescript --globalTypesFile=src/graphql/types/graphql-global-types.ts --outputFlat src/graphql/types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run types:check && npm run prettify && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint"
  },
  "dependencies": {
    "@apollo/client": "^3.3.8",
    "amazon-cognito-identity-js": "^4.5.10",
    "classnames": "^2.2.6",
    "graphql": "^15.5.0",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.3.1",
    "next": "^10.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.0.0-rc.2",
    "react-modal": "^3.12.1",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.18",
    "@svgr/webpack": "^5.5.0",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "apollo": "^2.32.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.0-beta1",
    "css-loader": "^5.0.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3"
  }
}
