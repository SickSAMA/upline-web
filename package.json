{
  "name": "upline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:fix": "jest -u",
    "test:watch": "jest --watch",
    "types:check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint"
  },
  "dependencies": {
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "^0.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "sass": "^1.26.10",
    "typescript": "^3.9.7"
  }
}
